stackOver = read.csv(file.choose(),header=T)
head(stackOver)
attach(stackOver)
#install.packages("Matrix")
#install.packages("arules")
library(arules)
rules <- apriori(stackOver, parameter = list(supp=0.001, conf=0.1), appearance = list(rhs=c("Employee.Status=Freelance"), default="lhs"))
rules <- apriori(stackOver, parameter = list(supp=0.001, conf=0.1), appearance = list(rhs=c("EmpStatus=Freelance"), default="lhs"))
rules <- apriori(stackOver, parameter = list(supp=0.001, conf=0.1), appearance = list(rhs=c("Employment.Status=Freelance / Contractor"), default="lhs"))
rules.sorted <- sort(rules, by="lift")
inspect(rules.sorted)
rules <- apriori(stackOver, parameter = list(supp=0.001, conf=0.8), appearance = list(rhs=c("Employment.Status=Freelance / Contractor"), default="lhs"))
rules.sorted <- sort(rules, by="lift")
inspect(rules.sorted)
rules <- apriori(stackOver, parameter = list(supp=0.001, conf=0.7), appearance = list(rhs=c("Employment.Status=Freelance / Contractor"), default="lhs"))
rules.sorted <- sort(rules, by="lift")
inspect(rules.sorted)
head(stackOver)
rules <- apriori(stackOver, parameter = list(supp=0.001, conf=0.7), appearance = list(rhs=c("Prgm.Hours=5-10 hours per week"), default="lhs"))
rules.sorted <- sort(rules, by="lift")
inspect(rules.sorted)
rules <- apriori(stackOver, parameter = list(supp=0.001, conf=0.5), appearance = list(rhs=c("Prgm.Hours=5-10 hours per week"), default="lhs"))
rules.sorted <- sort(rules, by="lift")
inspect(rules.sorted)
plot(rules.pruned)
library(arulesViz)
plot(rules.pruned)
